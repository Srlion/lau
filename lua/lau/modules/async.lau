    fn Lau.__ASYNC__(func) {
        ... => {
            if coroutine_running() {
                return func(...);
            } else {
                let rets_n, rets = pack(
                    coroutine_resume(
                        coroutine_create(func),
                        ...
                    )
                );

                if !rets[1] {
                    error(rets[2], 0);
                } else {
                    return unpack(rets, 2, rets_n);
                }
            }
        }
    }
